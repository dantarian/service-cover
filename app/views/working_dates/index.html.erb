<p id="notice"><%= notice %></p>

<h1>Working Dates</h1>

<table>
  <thead>
    <tr>
      <th>Working date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @working_dates.each do |working_date| %>
      <tr>
        <td><%= working_date.working_date %></td>
        <td><%= link_to 'Show', working_date %></td>
        <td><%= link_to 'Edit', edit_working_date_path(working_date) %></td>
        <td><%= link_to 'Destroy', working_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Working Date', new_working_date_path %>
